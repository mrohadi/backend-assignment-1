<body>
    <h1> Book List</h1>
    <a href="/books/add"> Add Book </a>
    <table>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Author</th>
            <th>Year Written</th>
            <th>Edition</th>
            <th>Price</th>
        </tr>
        <% data.forEach(item => { %>
            <tr>
                <td><%= item.id %></td>
                <td><%= item.title %></td>
                <td><%= item.author %></td>
                <td><%= item.year_written %></td>
                <td><%= item.edition %></td>
                <td><%= item.price %></td>
            </tr>
        <% }) %>
    </table>
</body>
